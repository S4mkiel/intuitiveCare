# Exemplo de configuração - renomeie para .env e ajuste os valores

# Configurações da aplicação
APP_PORT=5000               # Porta onde a aplicação irá rodar
APP_HOST=0.0.0.0            # Host onde a aplicação irá rodar
APP_DEBUG=True              # Modo debug (True/False)

# Configurações do arquivo CSV
CSV_FILE_PATH=Relatorio_cadop.csv  # Caminho para o arquivo CSV
CSV_SEPARATOR=;                   # Separador de colunas no CSV
CSV_ENCODING=utf-8                # Encoding do arquivo CSV

# Configurações de CORS
CORS_ENABLED=True                # Habilita/desabilita CORS (True/False)